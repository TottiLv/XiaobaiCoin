# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'XiaobaiCoin' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for XiaobaiCoin
  #网络请求
  pod 'Alamofire'
  
  #加载框
  pod 'SVProgressHUD'
  
  #自动约束布局
  pod 'SnapKit'
  
  #json转model
  pod 'HandyJSON'
  
  #下拉刷新，上拖加载更多
  pod 'ESPullToRefresh'
  
  #图片下载
  pod 'SDWebImage'
  
  pod 'SwiftyOpenCC'
  
end
